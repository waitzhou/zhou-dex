<resources>

    <!-- Base application theme. -->
    <style name="home_AppTheme" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/home_colorPrimary</item>
        <item name="colorPrimaryDark">@color/home_colorPrimaryDark</item>
        <item name="colorAccent">@color/home_colorAccent</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowBackground">@null</item>
    </style>

    <style name="home_FullScreen" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowTranslucentStatus">true</item>
    </style>

</resources>
