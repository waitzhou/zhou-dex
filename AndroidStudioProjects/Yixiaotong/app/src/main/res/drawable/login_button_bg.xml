<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape>
            <gradient
                android:angle="0"
                android:startColor="#ff0e7ef5"
                android:endColor="#ff695efd"
               />
            <corners android:radius="26dp" />
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="1dp" android:color="#ff0e7ef5" />
            <corners android:radius="26dp" />
        </shape>
    </item>
</selector>